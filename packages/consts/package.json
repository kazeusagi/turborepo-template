{
  "name": "@repo/consts",
  "version": "0.0.0",
  "private": true,
  "license": "MIT",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "files": [
    "./src/**"
  ],
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "import": "./src/index.ts",
      "require": "./src/index.ts"
    },
    "./*": {
      "import": "./src/*.ts",
      "require": "./src/*.ts"
    }
  },
  "devDependencies": {
    "@repo/typescript-config": "workspace:*",
    "@types/node": "^22.10.7",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.2",
    "typescript": "5.5.4"
  }
}
